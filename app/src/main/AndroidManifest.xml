<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tcs.maverick.talentdevelopment">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="com.tcs.maverick.talentdevelopment.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.ContactsActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity android:name="com.tcs.maverick.talentdevelopment.activities.DisplayCoursesActivity" />

        <activity android:name="com.tcs.maverick.talentdevelopment.activities.CourseDetailsActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.RegisteredCoursesActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.CompletedCoursesActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.MultipleDayRegisteredCoursesActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.RegisteredCoursesActivity" />


        <activity android:name="com.tcs.maverick.talentdevelopment.activities.FeedbackActivity1" />

        <activity android:name="com.tcs.maverick.talentdevelopment.activities.FeedbackActivity2" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.LeaderboardActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.LearningCategoriesActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.LearningAmbassadors"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.FaqsActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />


        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.LoadUpcomingSessions"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.LoadCompletedSessions"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.HomeActivity" />

        <activity android:name="com.tcs.maverick.talentdevelopment.activities.RegisteredUsers" />
        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.CompletedCoursesDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.tcs.maverick.talentdevelopment.activities.StudentFeedbackActivity"
            android:parentActivityName="com.tcs.maverick.talentdevelopment.activities.CompletedCoursesDetailsActivity" />


    </application>

    <uses-permission android:name="android.permission.CALL_PHONE" />


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <permission
        android:name="com.prgguru.example.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.prgguru.example.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />


</manifest>
